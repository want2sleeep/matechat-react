{
  "gitSiteUrl": "https://github.com/DevCloudFE/matechat-react/",
  "changeTags": {
    "feat": "New Features",
    "fix": "Bug Fixes",
    "perf": "Performance Improvements",
    "chore": "Chores",
    "refactor": "Refactors"
  },
  "pkgManagers": {
    "javascript": {
      "version": true,
      "getPublishedVersion": {
        "use": "fetch:check",
        "options": {
          "url": "https://registry.npmjs.com/${ pkg.pkg }/${ pkg.pkgFile.version }"
        }
      },
      "publish": ["npm publish --provenance --access public"]
    }
  },
  "packages": {
    "@matechat/react": {
      "path": ".",
      "manager": "javascript"
    }
  }
}
